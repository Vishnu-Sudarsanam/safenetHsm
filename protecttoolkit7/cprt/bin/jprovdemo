PTKPATH=/opt/safenet/protecttoolkit7/ptk
JAR=$PTKPATH/lib/jprovdemo.jar
OPTIONS=""

ostype=`uname -s`
case "$ostype" in
    [lL]inux)
        LD_LIBRARY_PATH=$PTKPATH/lib:$LD_LIBRARY_PATH
        export LD_LIBRARY_PATH
        ;;
    HP-UX)
        OPTIONS="-Xint"
        ;;
esac

CLASSPATH=$PTKPATH/lib/jprov_sfnt.jar:$CLASSPATH
export CLASSPATH

java $OPTIONS -jar $JAR $*
